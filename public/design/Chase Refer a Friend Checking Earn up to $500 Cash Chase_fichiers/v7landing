window.onload=function(){function t(n){var i=!1,t=$(n.data("or"));n.data("first-entry",!1);n.val(n.val().trim());switch(n.data("type")){case"num":if(n.val().length<parseInt(n.attr("maxlength"))||isNaN(n.val())){if(t.length>0){i=t.val().length<parseInt(t.attr("maxlength"))||isNaN(t.val());break}i=!0;break}case"alpha":default:if(n.val().length===0){if(t.length>0){i=t.val().length===0;break}i=!0;break}}return i?(r(n),t.length>0&&r(t),!1):(u(n),t.length>0&&u(t),!0)}function r(n){n.attr("aria-invalid","true")}function u(n){n.attr("aria-invalid","false")}var f=0,n=!1,e=$(".form-input"),i;e.each(function(){$(this).data("first-entry",!0).on("input",function(n){$(n.target).data("firstEntry")||t($(n.target))}).on("blur",function(n){$(n.target).data("firstEntry",!1);t($(n.target))})});$("#authForm").submit(function(t){if(!n)return t.preventDefault(),!1});$(".email-submit").on("click",function(){n=!0;e.each(function(){n?n=t($(this)):t($(this))});$(".DebitCard").val().length>0&&$(".CheckingAccount").val().length>0&&(n=!1);n?($(".error-box").removeClass("-visible"),$(".links-box").addClass("-visible")):(f++,f<3?($("#try-later").removeClass("-visible"),$("#warningMsg").removeClass("-visible"),$("#authStatus").val()==1?($("#field-error-fname").addClass("-visible"),$("#field-error").removeClass("-visible")):($("#field-error-fname").removeClass("-visible"),$("#field-error").addClass("-visible"))):($("#field-error").removeClass("-visible"),$("#field-error-fname").removeClass("-visible"),$("#try-later").addClass("-visible"),$("#warningMsg").removeClass("-visible")))});($("#LastName").val()&&$("label[for='last-name']").addClass("-to-top"),$("#ZipCode").val()&&$("label[for='zip']").addClass("-to-top"),$(".DebitCard").val()&&$("label[for='last-four-digits-debit']").addClass("-to-top"),$(".CheckingAccount").val()&&$("label[for='last-four-digits-checking']").addClass("-to-top"),$("#FirstName").val()&&$("label[for='first-name']").addClass("-to-top"),i=$("#authStatus").val(),i)&&i==1&&$("#FirstName").val().length==0&&($("#warningMsg").addClass("-visible"),$("#FirstName").focus())};$(document).ready(function(){var n=$("#authStatus").val();$("#LastName").val().length>0&&$("#ZipCode").val().length>0&&($(".DebitCard").val().length>0||$(".CheckingAccount").val().length>0)&&($(".email-submit").attr("disabled",!1),$(".email-submit").css("background-color","#178404"));$(".form-input").on("keyup",function(){let n=!0;$("#FirstName").length?$("#FirstName").val().length>0&&$("#LastName").val().length>0&&$("#ZipCode").val().length>0&&($(".DebitCard").val().length>0||$(".CheckingAccount").val().length>0)&&(n=!1):$("#LastName").val().length>0&&$("#ZipCode").val().length>0&&($(".DebitCard").val().length>0||$(".CheckingAccount").val().length>0)&&(n=!1);n?($(".email-submit").prop("disabled",!0),$(".email-submit").css("background-color","#808080")):($(".email-submit").attr("disabled",!1),$(".email-submit").css("background-color","#178404"))});n==1&&$("#FirstName").val().length==0&&($(".email-submit").prop("disabled",!0),$(".email-submit").css("background-color","#808080"))})